<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE web-app
    PUBLIC "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
    "http://java.sun.com/dtd/web-app_2_3.dtd">
<web-app>

	<!-- Default page to serve -->
	<welcome-file-list>
		<welcome-file>SoftwareEngineering_team_13.html</welcome-file>
	</welcome-file-list>
	
	<!-- Servlets -->
  <servlet>
    <servlet-name>dataServiceImpl</servlet-name>
    <servlet-class>sofwareengineering.team.thirteen.gwt.webapp.server.DataServiceImpl</servlet-class>
  </servlet>

  <servlet-mapping>
    <servlet-name>dataServiceImpl</servlet-name>
    <url-pattern>/softwareengineering_team_13/data</url-pattern>
  </servlet-mapping>
  
</web-app>
